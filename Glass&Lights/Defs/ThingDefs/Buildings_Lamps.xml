<Buildings>


  <ThingDef Name="BuildingBase" Abstract="True">
    <category>Building</category>
    <thingClass>Building</thingClass>
    <soundImpactDefault>BulletImpactMetal</soundImpactDefault>
    <selectable>true</selectable>
    <drawerType>MapMeshAndRealTime</drawerType>
    <terrainAffordanceNeeded>Light</terrainAffordanceNeeded>
    <repairEffect>Repair</repairEffect>
    <leaveResourcesWhenKilled>true</leaveResourcesWhenKilled>
    <filthLeaving>BuildingRubble</filthLeaving>
  </ThingDef>
  
  <!--======================================CeilingLights======================================-->
  
  <ThingDef ParentName="BuildingBase">
    <defName>CeilingLampYellow</defName>
    <label>ceiling lamp (yellow)</label>
    <thingClass>Building</thingClass>
    <category>Building</category>
    <graphicData>
      <texPath>Things/Building/Lights/CeilingLamp_Yellow</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shadowInfo>
        <basewidth>0.3</basewidth>
        <baseHeight>0.3</baseHeight>
        <tallness>0.6</tallness>
        <offset>(0,0,-0.1)</offset>
      </shadowInfo>
	  <shaderType>Transparent</shaderType>
    </graphicData>
    <altitudeLayer>BuildingTall</altitudeLayer>
    <passability>PassThroughOnly</passability>
    <statBases>
      <MaxHitPoints>35</MaxHitPoints>
      <WorkToMake>85</WorkToMake>
      <Flammability>1.0</Flammability>
	  <Beauty>20</Beauty>
    </statBases>
    <selectable>true</selectable>
    <description>A yellow ceiling lamp that lights an area in a dull yellow color.</description>
    <costList>
		<LargeCircularGlass>1</LargeCircularGlass>
        <Steel>8</Steel>
    </costList>
    <soundImpactDefault>BulletImpactMetal</soundImpactDefault>
    <comps>
      <li>
        <compClass>CompPowerTrader</compClass>
        <basePowerConsumption>100</basePowerConsumption>
        <shortCircuitInRain>false</shortCircuitInRain>
        <soundPowerOn>PowerOnSmall</soundPowerOn>
        <soundPowerOff>PowerOffSmall</soundPowerOff>
      </li>
      <li>
        <compClass>CompGlower</compClass>
        <glowRadius>13</glowRadius>
        <glowColor>(217,217,0,0)</glowColor>
      </li>
	  <li Class="CommunityCoreLibrary.RestrictedPlacement_Properties">
			<compClass>CommunityCoreLibrary.RestrictedPlacement_Comp</compClass>
      </li>
    </comps>
	<tickerType>Rare</tickerType>
	<placeWorkers>
      <li>CommunityCoreLibrary.PlaceWorker_OnlyUnderRoof</li>
    </placeWorkers>
    <designationCategory>Furniture</designationCategory>
    <rotatable>false</rotatable>
    <designationHotKey></designationHotKey>
	<filthLeaving>BrokenGlass</filthLeaving>
	<leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>
  </ThingDef>
  
